-- Easter Egg Section (Bottom right)
local EasterEggSection = Tabs.Settings:Section({Text = "???"})

EasterEggSection:Button({
    Text = "DO NOT PRESS",
    Tooltip = "Seriously, don't do it...",
    Callback = function()
        print("FREE FORTNITE FEET")
        
        -- Screen shake effect
        local Camera = game.Workspace.CurrentCamera
        local Player = game.Players.LocalPlayer
        local Character = Player.Character
        
        -- Intense screen shake
        spawn(function()
            for i = 1, 50 do
                Camera.CFrame = Camera.CFrame * CFrame.Angles(
                    math.rad(math.random(-5, 5)),
                    math.rad(math.random(-5, 5)),
                    math.rad(math.random(-5, 5))
                )
                wait(0.05)
            end
        end)
        
        -- Rainbow explosion on screen
        local ScreenGui = Instance.new("ScreenGui")
        ScreenGui.Parent = game:GetService("CoreGui")
        ScreenGui.Name = "VortexEasterEgg"
        
        for i = 1, 20 do
            local Frame = Instance.new("Frame")
            Frame.Parent = ScreenGui
            Frame.Size = UDim2.new(0, math.random(50, 200), 0, math.random(50, 200))
            Frame.Position = UDim2.new(math.random(0, 100) / 100, 0, math.random(0, 100) / 100, 0)
            Frame.BackgroundColor3 = Color3.fromHSV(math.random(), 1, 1)
            Frame.BorderSizePixel = 0
            Frame.BackgroundTransparency = 0.3
            
            spawn(function()
                for j = 1, 30 do
                    Frame.Rotation = Frame.Rotation + 10
                    Frame.BackgroundColor3 = Color3.fromHSV(math.random(), 1, 1)
                    wait(0.05)
                end
                Frame:Destroy()
            end)
        end
        
        -- Explosion text
        local TextLabel = Instance.new("TextLabel")
        TextLabel.Parent = ScreenGui
        TextLabel.Size = UDim2.new(1, 0, 1, 0)
        TextLabel.Position = UDim2.new(0, 0, 0, 0)
        TextLabel.BackgroundTransparency = 1
        TextLabel.Text = "sup"
        TextLabel.TextSize = 100
        TextLabel.Font = Enum.Font.GothamBold
        TextLabel.TextColor3 = Color3.fromRGB(255, 50, 255)
        TextLabel.TextStrokeTransparency = 0
        TextLabel.TextStrokeColor3 = Color3.fromRGB(0, 0, 0)
        TextLabel.ZIndex = 10
        
        -- Make character spin
        if Character and Character:FindFirstChild("HumanoidRootPart") then
            local HRP = Character.HumanoidRootPart
            spawn(function()
                for i = 1, 36 do
                    HRP.CFrame = HRP.CFrame * CFrame.Angles(0, math.rad(10), 0)
                    wait(0.05)
                end
            end)
        end
        
        -- Confetti particles
        if Character and Character:FindFirstChild("HumanoidRootPart") then
            local Attachment = Instance.new("Attachment")
            Attachment.Parent = Character.HumanoidRootPart
            
            local Particle = Instance.new("ParticleEmitter")
            Particle.Parent = Attachment
            Particle.Texture = "rbxasset://textures/particles/smoke_main.dds"
            Particle.Rate = 100
            Particle.Lifetime = NumberRange.new(2, 3)
            Particle.Speed = NumberRange.new(10, 20)
            Particle.SpreadAngle = Vector2.new(180, 180)
            Particle.Color = ColorSequence.new({
                ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 0, 0)),
                ColorSequenceKeypoint.new(0.5, Color3.fromRGB(0, 255, 0)),
                ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 0, 255))
            })
            
            wait(2.5)
            Particle.Enabled = false
            wait(3)
            Attachment:Destroy()
        end
        
        wait(2.5)
        ScreenGui:Destroy()
        
        print("borinngg")
    end
})
