Tabs.Universal:Select()

-- Settings Tab
local ThemeSection = Tabs.Settings:Section({Text = "Theme Settings"})
local ConfigSection = Tabs.Settings:Section({Text = "Config Management"})
local AntiCheatSection = Tabs.Settings:Section({Text = "Anti-Cheat Detector"})
local MiscSettingsSection = Tabs.Settings:Section({Text = "Misc Settings"})

-- Theme Settings
local UILibrary = nil
local MainFrame = nil

-- Find the library's main frame
for _, obj in pairs(game:GetService("CoreGui"):GetDescendants()) do
    if obj.Name == "Shaman" or (obj:IsA("ScreenGui") and obj:FindFirstChild("Main")) then
        MainFrame = obj:FindFirstChild("Main") or obj
        break
    end
end

ThemeSection:Dropdown({
    Text = "Theme Color",
    List = {"Blue", "Red", "Green", "Purple", "Orange", "Pink", "Cyan", "Yellow"},
    Callback = function(v)
        local colors = {
            Blue = Color3.fromRGB(0, 150, 255),
            Red = Color3.fromRGB(255, 50, 50),
            Green = Color3.fromRGB(50, 255, 100),
            Purple = Color3.fromRGB(150, 50, 255),
            Orange = Color3.fromRGB(255, 150, 50),
            Pink = Color3.fromRGB(255, 100, 200),
            Cyan = Color3.fromRGB(50, 200, 255),
            Yellow = Color3.fromRGB(255, 220, 50)
        }
        
        local selectedColor = colors[v] or colors.Blue
        
        if MainFrame then
            for _, obj in pairs(MainFrame:GetDescendants()) do
                if obj:IsA("Frame") or obj:IsA("TextButton") or obj:IsA("TextLabel") then
                    if obj.BorderColor3 then
                        obj.BorderColor3 = selectedColor
                    end
                    if obj:IsA("TextLabel") or obj:IsA("TextButton") then
                        -- Never set to gray
                        if obj.Name ~= "Title" and obj.TextColor3 ~= Color3.fromRGB(255, 255, 255) then
                            obj.TextColor3 = selectedColor
                        end
                    end
                    -- Ensure backgrounds are never gray
                    if obj.BackgroundColor3 == Color3.fromRGB(128, 128, 128) then
                        obj.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
                    end
                end
                if obj:IsA("UIStroke") then
                    obj.Color = selectedColor
                end
            end
        end
    end
})

ThemeSection:Toggle({
    Text = "Auto-Save Theme",
    Tooltip = "Automatically save theme preferences",
    Callback = function(state)
        _G.AutoSaveTheme = state
        if state then
            print("Theme auto-save enabled!")
        else
            print("Theme auto-save disabled!")
        end
    end
})

-- Config Management
local ConfigName = ""

ConfigSection:Button({
    Text = "Save Config",
    Tooltip = "Save current settings (use default name)",
    Callback = function()
        ConfigName = "VortexConfig_" .. tostring(os.time())
        
        local config = {}
        for flag, value in pairs(Flags) do
            config[flag] = value
        end
        
        local success, err = pcall(function()
            writefile(ConfigName..".txt", game:GetService("HttpService"):JSONEncode(config))
        end)
        
        if success then
            print("Config saved: " .. ConfigName)
        else
            warn("Failed to save config: " .. tostring(err))
        end
    end
})

ConfigSection:Button({
    Text = "Load Latest Config",
    Tooltip = "Load most recent saved settings",
    Callback = function()
        local files = {}
        local success = pcall(function()
            files = listfiles()
        end)
        
        if success and #files > 0 then
            for _, file in pairs(files) do
                if file:find("VortexConfig_") and file:find(".txt") then
                    local content = readfile(file)
                    local config = game:GetService("HttpService"):JSONDecode(content)
                    for flag, value in pairs(config) do
                        Flags[flag] = value
                    end
                    print("Config loaded: " .. file)
                    return
                end
            end
            warn("No config files found")
        else
            warn("Failed to load config")
        end
    end
})

-- Anti-Cheat Detector
local ACLabel = AntiCheatSection:Label({
    Text = "Click 'Scan' to detect anti-cheat",
    Color = Color3.fromRGB(255, 255, 100)
})

local function detectAntiCheat()
    local score = 0
    local maxScore = 100
    local detections = {}
    
    -- Whitelist of common Roblox objects to ignore
    local whitelist = {
        "rbxcharactersounds", "rbxanimations", "rbxinternal", "humanoid", "animate",
        "health", "control", "camerascript", "playergui", "backpack", "starter"
    }
    
    local function isWhitelisted(name)
        local lower = name:lower()
        for _, whitelisted in pairs(whitelist) do
            if lower:find(whitelisted) then
                return true
            end
        end
        return false
    end
    
    -- Check for common anti-cheat script names (High priority)
    local acScriptNames = {"anticheat", "anti_cheat", "antikick", "antiexploit", "cheatdetect", "exploitcheck"}
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("LocalScript") or obj:IsA("Script") or obj:IsA("ModuleScript") then
            local name = obj.Name:lower()
            if not isWhitelisted(name) then
                for _, acName in pairs(acScriptNames) do
                    if name == acName or name:find("^" .. acName) then
                        score = score + 20
                        table.insert(detections, "AC Script: " .. obj.Name .. " in " .. obj.Parent.Name)
                        break
                    end
                end
            end
        end
    end
    
    -- Check for anti-cheat folders/services
    local suspiciousPaths = {
        game:GetService("ReplicatedStorage"),
        game:GetService("ReplicatedFirst"),
        game.Workspace
    }
    
    for _, location in pairs(suspiciousPaths) do
        local success = pcall(function()
            for _, obj in pairs(location:GetChildren()) do
                local name = obj.Name:lower()
                if not isWhitelisted(name) then
                    if name:find("anticheat") or name:find("anti%-cheat") or name:find("security") and name:find("system") then
                        score = score + 15
                        table.insert(detections, "AC Folder: " .. obj.Name .. " in " .. location.Name)
                    end
                end
            end
        end)
    end
    
    -- Check RemoteEvent/Function with suspicious names only
    local suspiciousRemotes = 0
    local suspiciousNames = {"flag", "report", "log", "ban", "kick", "detect", "check", "verify"}
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("RemoteEvent") or obj:IsA("RemoteFunction") then
            local name = obj.Name:lower()
            if not isWhitelisted(name) then
                for _, susName in pairs(suspiciousNames) do
                    if name:find(susName) then
                        suspiciousRemotes = suspiciousRemotes + 1
                        table.insert(detections, "Suspicious Remote: " .. obj.Name)
                        break
                    end
                end
            end
        end
    end
    
    if suspiciousRemotes > 5 then
        score = score + 20
        table.insert(detections, "Multiple suspicious remotes: " .. suspiciousRemotes)
    elseif suspiciousRemotes > 2 then
        score = score + 10
    end
    
    -- Check for speed/position monitoring
    for _, obj in pairs(game:GetDescendants()) do
        if obj:IsA("Script") or obj:IsA("LocalScript") then
            local success, source = pcall(function() return obj.Source end)
            if success and source and #source > 100 then
                local lower = source:lower()
                local hasSpeedCheck = lower:find("walkspeed") and (lower:find("kick") or lower:find("ban"))
                local hasPosCheck = lower:find("magnitude") and lower:find("rootpart") and (lower:find("kick") or lower:find("ban"))
                
                if hasSpeedCheck or hasPosCheck then
                    score = score + 25
                    table.insert(detections, "Movement monitoring in: " .. obj.Name)
                    break
                end
            end
        end
    end
    
    -- Check for popular anti-cheat systems
    local knownACs = {
        {name = "Adonis", score = 30},
        {name = "BasicAdmin", score = 15},
        {name = "Sentinel", score = 35},
        {name = "Proton", score = 30},
        {name = "Deflect", score = 35}
    }
    
    for _, ac in pairs(knownACs) do
        for _, obj in pairs(game:GetDescendants()) do
            if obj.Name == ac.name or (obj.Name:lower():find(ac.name:lower()) and not isWhitelisted(obj.Name)) then
                score = score + ac.score
                table.insert(detections, "Known AC System: " .. ac.name)
                break
            end
        end
    end
    
    -- Cap at 100%
    score = math.min(score, maxScore)
    
    -- Determine color and status
    local color = Color3.fromRGB(100, 255, 100) -- Green (Safe)
    local status = "Safe - Low detection risk"
    
    if score >= 20 and score < 40 then
        color = Color3.fromRGB(150, 255, 100) -- Light green
        status = "Mostly Safe - Minor detection"
    elseif score >= 40 and score < 60 then
        color = Color3.fromRGB(255, 255, 100) -- Yellow
        status = "Moderate - Use with caution"
    elseif score >= 60 and score < 80 then
        color = Color3.fromRGB(255, 180, 50) -- Orange
        status = "High Risk - Be careful"
    elseif score >= 80 then
        color = Color3.fromRGB(255, 50, 50) -- Red
        status = "EXTREME RISK - Avoid cheating!"
    end
    
    local statusText = "Anti-Cheat Strength: " .. score .. "%\n" .. status
    
    ACLabel:Set({
        Text = statusText,
        Color = color
    })
    
    print("=== Anti-Cheat Scan Results ===")
    print("Score: " .. score .. "%")
    print("Status: " .. status)
    print("Detections found: " .. #detections)
    for i, detection in ipairs(detections) do
        print(i .. ". " .. detection)
    end
    print("================================")
end

AntiCheatSection:Button({
    Text = "Scan Anti-Cheat",
    Tooltip = "Detect game anti-cheat",
    Callback = function()
        detectAntiCheat()
    end
})

AntiCheatSection:Toggle({
    Text = "Auto-Scan on Join",
    Callback = function(state)
        if state then
            detectAntiCheat()
        end
    end
})

-- Misc Settings
MiscSettingsSection:Toggle({
    Text = "FPS Counter",
    Callback = function(state)
        if state then
            _G.ShowFPS = true
            local fpsLabel = Instance.new("TextLabel")
            fpsLabel.Name = "FPSCounter"
            fpsLabel.Parent = game:GetService("CoreGui"):WaitForChild("RobloxGui")
            fpsLabel.Size = UDim2.new(0, 100, 0, 30)
            fpsLabel.Position = UDim2.new(1, -110, 0, 10)
            fpsLabel.BackgroundTransparency = 0.5
            fpsLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            fpsLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            fpsLabel.Font = Enum.Font.Code
            fpsLabel.TextSize = 18
            
            spawn(function()
                while _G.ShowFPS do
                    local fps = math.floor(1 / game:GetService("RunService").RenderStepped:Wait())
                    fpsLabel.Text = "FPS: " .. fps
                    wait(0.5)
                end
            end)
        else
            _G.ShowFPS = false
            local fpsLabel = game:GetService("CoreGui"):WaitForChild("RobloxGui"):FindFirstChild("FPSCounter")
            if fpsLabel then
                fpsLabel:Destroy()
            end
        end
    end
})

MiscSettingsSection:Toggle({
    Text = "Ping Display",
    Callback = function(state)
        if state then
            _G.ShowPing = true
            local pingLabel = Instance.new("TextLabel")
            pingLabel.Name = "PingCounter"
            pingLabel.Parent = game:GetService("CoreGui"):WaitForChild("RobloxGui")
            pingLabel.Size = UDim2.new(0, 100, 0, 30)
            pingLabel.Position = UDim2.new(1, -110, 0, 45)
            pingLabel.BackgroundTransparency = 0.5
            pingLabel.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
            pingLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
            pingLabel.Font = Enum.Font.Code
            pingLabel.TextSize = 18
            
            spawn(function()
                while _G.ShowPing do
                    local ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
                    pingLabel.Text = "Ping: " .. ping
                    wait(0.5)
                end
            end)
        else
            _G.ShowPing = false
            local pingLabel = game:GetService("CoreGui"):WaitForChild("RobloxGui"):FindFirstChild("PingCounter")
            if pingLabel then
                pingLabel:Destroy()
            end
        end
    end
})

MiscSettingsSection:Toggle({
    Text = "Performance Mode",
    Tooltip = "Reduce graphics for better FPS",
    Callback = function(state)
        local lighting = game:GetService("Lighting")
        if state then
            settings().Rendering.QualityLevel = 1
            lighting.GlobalShadows = false
            lighting.FogEnd = 9e9
            
            for _, obj in pairs(game.Workspace:GetDescendants()) do
                if obj:IsA("ParticleEmitter") or obj:IsA("Trail") then
                    obj.Enabled = false
                end
                if obj:IsA("Explosion") then
                    obj.BlastPressure = 1
                    obj.BlastRadius = 1
                end
            end
        else
            settings().Rendering.QualityLevel = Enum.QualityLevel.Automatic
            lighting.GlobalShadows = true
        end
    end
})

MiscSettingsSection:Button({
    Text = "Anti-AFK",
    Tooltip = "Prevent being kicked for idling",
    Callback = function()
        local vu = game:GetService("VirtualUser")
        game:GetService("Players").LocalPlayer.Idled:connect(function()
            vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            wait(1)
            vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        end)
        print("Anti-AFK enabled!")
    end
})

