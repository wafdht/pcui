    Text = "FOV Changer",
    Default = 70,
    Minimum = 30,
    Maximum = 120,
    Flag = "FOVSlider",
    Callback = function(Value)
        game.Workspace.CurrentCamera.FieldOfView = Value
    end
})

-- Server Info Section
local ServerInfoSection = Tabs.Settings:Section({Text = "Server Information"})

local ServerInfoLabel = ServerInfoSection:Label({
    Text = "Loading server info...",
    Color = Color3.fromRGB(100, 200, 255)
})

ServerInfoSection:Button({
    Text = "Refresh Server Info",
    Tooltip = "Update server information",
    Callback = function()
        local Players = game:GetService("Players")
        local Stats = game:GetService("Stats")
        
        local playerCount = #Players:GetPlayers()
        local maxPlayers = Players.MaxPlayers
        local ping = Stats.Network.ServerStatsItem["Data Ping"]:GetValueString()
        local jobId = game.JobId
        local placeId = game.PlaceId
        
        local infoText = string.format(
            "Players: %d/%d\nPing: %s\nPlace ID: %s\nJob ID: %s",
            playerCount, maxPlayers, ping, placeId, jobId:sub(1, 8) .. "..."
        )
        
        ServerInfoLabel:Set({
            Text = infoText,
            Color = Color3.fromRGB(100, 255, 100)
        })
    end
})

ServerInfoSection:Dropdown({
    Text = "Teleport to Player",
    List = {},
    Callback = function(playerName)
        local targetPlayer = game.Players:FindFirstChild(playerName)
        if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
            local localPlayer = game.Players.LocalPlayer
            if localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart") then
                localPlayer.Character.HumanoidRootPart.CFrame = targetPlayer.Character.HumanoidRootPart.CFrame
                print("Teleported to " .. playerName)
            end
        end
    end
})

-- Update player list for dropdown
spawn(function()
    while wait(5) do
        local playerList = {}
        for _, player in pairs(game.Players:GetPlayers()) do
            if player ~= game.Players.LocalPlayer then
                table.insert(playerList, player.Name)
            end
        end
        -- Note: Dropdown refresh might not work in Shaman, this is a placeholder
    end
end)

-- Exploit Detector Section
local ExploitDetectorSection = Tabs.Settings:Section({Text = "Exploit Detector"})

local ExploitLabel = ExploitDetectorSection:Label({
    Text = "Click 'Scan' to detect exploiters",
    Color = Color3.fromRGB(255, 200, 100)
})

ExploitDetectorSection:Button({
    Text = "Scan for Exploiters",
    Tooltip = "Detect suspicious players",
    Callback = function()
        local suspiciousPlayers = {}
        local Players = game:GetService("Players")
        local LocalPlayer = Players.LocalPlayer
        
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character then
                local humanoid = player.Character:FindFirstChildOfClass("Humanoid")
                local hrp = player.Character:FindFirstChild("HumanoidRootPart")
                
                if humanoid and hrp then
                    -- Check for suspicious speed
                    if humanoid.WalkSpeed > 50 then
                        table.insert(suspiciousPlayers, player.Name .. " (High Speed)")
                    end
                    
                    -- Check for suspicious jump
                    if humanoid.JumpPower > 100 or humanoid.JumpHeight > 15 then
                        table.insert(suspiciousPlayers, player.Name .. " (High Jump)")
                    end
                    
                    -- Check for flying (velocity check)
                    if hrp.AssemblyLinearVelocity.Y > 50 and humanoid:GetState() ~= Enum.HumanoidStateType.Freefall then
                        table.insert(suspiciousPlayers, player.Name .. " (Flying)")
                    end
                end
            end
        end
        
        if #suspiciousPlayers > 0 then
            local resultText = "⚠️ Suspicious Players:\n" .. table.concat(suspiciousPlayers, "\n")
            ExploitLabel:Set({
                Text = resultText,
                Color = Color3.fromRGB(255, 100, 100)
            })
            print("=== Exploit Scan Results ===")
            for _, sus in ipairs(suspiciousPlayers) do
                print(sus)
            end
        else
            ExploitLabel:Set({
                Text = "✅ No suspicious players detected",
                Color = Color3.fromRGB(100, 255, 100)
            })
        end
    end
})

-- Fun Features Section
local FunSection = Tabs.Settings:Section({Text = "Fun Features"})

FunSection:Toggle({
    Text = "Fake Lag",
    Tooltip = "Make it look like you're lagging",
    Callback = function(state)
        _G.FakeLag = state
        if state then
            spawn(function()
                while _G.FakeLag do
                    local character = game.Players.LocalPlayer.Character
                    if character and character:FindFirstChild("HumanoidRootPart") then
                        local hrp = character.HumanoidRootPart
                        local oldPos = hrp.CFrame
                        wait(math.random(1, 3))
                        if _G.FakeLag then
                            hrp.CFrame = oldPos
                        end
                    end
                    wait(0.1)
                end
            end)
        end
    end
})

FunSection:Button({
    Text = "Remove Textures",
    Tooltip = "Remove all textures for FPS boost",
    Callback = function()
        for _, obj in pairs(game.Workspace:GetDescendants()) do
            if obj:IsA("Decal") or obj:IsA("Texture") then
                obj.Transparency = 1
            end
            if obj:IsA("BasePart") then
                obj.Material = Enum.Material.SmoothPlastic
            end
        end
        print("Textures removed!")
    end
})

